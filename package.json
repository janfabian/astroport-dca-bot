{
  "name": "@janfabian/astroport-dca-bot",
  "version": "1.0.0beta4",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc --build && chmod +x ./dist/index.js",
    "bot": "NODE_OPTIONS=--openssl-legacy-provider NODE_NO_WARNINGS=1  node ./dist",
    "serve": "NODE_OPTIONS=--openssl-legacy-provider NODE_NO_WARNINGS=1 ts-node src/index.ts",
    "lint": "tsc --noEmit && eslint \"**/*.ts\" --quiet --fix",
    "test": "NODE_OPTIONS=--openssl-legacy-provider jest",
    "prepublish": "npm run build"
  },
  "bin": "./dist/index.js",
  "author": "jan fabian",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@types/jest": "29.5.2",
    "@types/node": "18.16.19",
    "@types/ramda": "0.29.3",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.6.1",
    "prettier": "3.0.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "dependencies": {
    "@terra-money/terra.js": "3.1.9",
    "commander": "9.5.0",
    "dotenv": "16.3.1",
    "ramda": "0.29.0"
  }
