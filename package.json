{
  "name": "@janfabian/astroport-dca-bot",
  "version": "1.0.0beta4",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc --build && chmod +x ./dist/index.js",
    "bot": "NODE_OPTIONS=--openssl-legacy-provider NODE_NO_WARNINGS=1  node ./dist",
    "serve": "NODE_OPTIONS=--openssl-legacy-provider NODE_NO_WARNINGS=1 ts-node src/index.ts",
    "lint": "tsc --noEmit && eslint \"**/*.ts\" --quiet --fix",
    "test": "NODE_OPTIONS=--openssl-legacy-provider jest",
    "prepublish": "npm run build"
  },
  "bin": "./dist/index.js",
  "author": "jan fabian",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@types/jest": "29.2.6",
    "@types/node": "18.11.18",
    "@types/ramda": "0.28.21",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "eslint": "8.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.3.1",
    "prettier": "2.8.3",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "dependencies": {
    "@terra-money/terra.js": "3.1.7",
    "commander": "9.5.0",
    "dotenv": "16.0.3",
    "ramda": "0.28.0"
  }
