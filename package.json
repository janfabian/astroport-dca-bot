{
  "name": "@janfabian/astroport-dca-bot",
  "version": "1.0.0beta4",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc --build && chmod +x ./dist/index.js",
    "bot": "NODE_OPTIONS=--openssl-legacy-provider NODE_NO_WARNINGS=1  node ./dist",
    "serve": "NODE_OPTIONS=--openssl-legacy-provider NODE_NO_WARNINGS=1 ts-node src/index.ts",
    "lint": "tsc --noEmit && eslint \"**/*.ts\" --quiet --fix",
    "test": "NODE_OPTIONS=--openssl-legacy-provider jest",
    "prepublish": "npm run build"
  },
  "bin": "./dist/index.js",
  "author": "jan fabian",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@types/jest": "29.5.14",
    "@types/node": "22.13.8",
    "@types/ramda": "0.30.2",
    "@typescript-eslint/eslint-plugin": "8.31.1",
    "@typescript-eslint/parser": "8.31.1",
    "eslint": "9.26.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-prettier": "5.3.1",
    "jest": "29.7.0",
    "prettier": "3.5.3",
    "ts-jest": "29.3.2",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "dependencies": {
    "@terra-money/terra.js": "3.1.10",
